port 8080; #Another comment

path /static StaticHandler {
    root www/;
}
path /proxy ReverseProxyHandler {
    remote_host http://www.ucla.edu;
    remote_port 80;
}

path /proxy1 ReverseProxyHandler {
    remote_host http://www.ucla.edu/asdf;
    remote_port 80;
}

path /echo EchoHandler {}

path /status StatusHandler {}

# Default handler if no handlers match
default NotFoundHandler {}

